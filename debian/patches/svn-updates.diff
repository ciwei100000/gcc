# DP: updates from the 4.9 branch upto 2016xxxx (r238903).

last_update()
{
	cat > ${dir}LAST_UPDATED <EOF
Sat Jul 30 08:52:21 CEST 2016
Sat Jul 30 06:52:21 UTC 2016 (revision 238903)
EOF
}

LANG=C svn diff svn://gcc.gnu.org/svn/gcc/tags/gcc_4_9_4_release svn://gcc.gnu.org/svn/gcc/branches/gcc-4_9-branch \
	| sed -r 's,^--- (\S+)\t(\S+)(.*)$,--- a/src/\1\t\2,;s,^\+\+\+ (\S+)\t(\S+)(.*)$,+++ b/src/\1\t\2,' \
	| awk '/^Index:.*\.(class|texi)/ {skip=1; next} /^Index:/ { skip=0 } skip==0'

