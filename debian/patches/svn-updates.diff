# DP: updates from the 4.6 branch upto 2013xxxx (r197516).

last_updated()
{
	cat > ${dir}LAST_UPDATED <<EOF
Fri Apr  5 16:06:37 CEST 2013
Fri Apr  5 14:06:37 UTC 2013 (revision 197516)
EOF
}

LANG=C svn diff svn://gcc.gnu.org/svn/gcc/tags/gcc_4_6_4_release svn://gcc.gnu.org/svn/gcc/branches/gcc-4_6-branch \
	| sed -r 's,^--- (\S+)\t(\S+)(.*)$,--- a/src/\1\t\2,;s,^\+\+\+ (\S+)\t(\S+)(.*)$,+++ b/src/\1\t\2,' \
	| awk '/^Index:.*\.(class|texi)/ {skip=1; next} /^Index:/ { skip=0 } skip==0'

